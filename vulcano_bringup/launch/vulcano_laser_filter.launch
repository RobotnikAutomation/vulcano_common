<?xml version="1.0"?>
<launch>
  <node pkg="laser_filters" type="scan_to_scan_filter_chain"
      name="front_laser_filter">
    <rosparam command="load" file="$(find vulcano_bringup)/config/vulcano_laser_config.yaml" />
    <remap from="scan" to="front_laser/scan" />
    <remap from="scan_filtered" to="front_laser/scan_filtered" />
  </node>

  <node pkg="laser_filters" type="scan_to_scan_filter_chain"
      name="rear_laser_filter">
    <rosparam command="load" file="$(find vulcano_bringup)/config/vulcano_laser_config.yaml" />
    <remap from="scan" to="rear_laser/scan" />
    <remap from="scan_filtered" to="rear_laser/scan_filtered" />
  </node>

</launch>


