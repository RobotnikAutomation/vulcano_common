<?xml version="1.0"?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find vulcano_control)/config/controllers.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="arm_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" args="left_arm_controller right_arm_controller --shutdown-timeout 1"/>                      
                        

</launch>

